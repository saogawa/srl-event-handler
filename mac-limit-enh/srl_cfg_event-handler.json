{
  "system": {
    "event-handler": {
      "instance": [
        {
          "name": "mac-limit-enh",
          "admin-state": "enable",
          "upython-script": "run-script.py",
          "paths": [
            "system information current-datetime"
          ],
          "options": {
            "object": [
              {
                "name": "debug",
                "value": "true"
              },
              {
                "name": "interval",
                "value": "30"
              },
              {
                "name": "script",
                "value": "/usr/bin/python3 /opt/srlinux/eventmgr/mac-limit-enh.py"
              }
            ]
          }
        }
      ]
    }
  }
}
